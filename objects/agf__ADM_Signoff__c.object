    <validationRules>
        <fullName>agf__Approver_must_be_Applications_Approver</fullName>
        <active>true</active>
        <description>The Approver must be an Application Approver of the Application to which  this Release is related.</description>
        <errorConditionFormula>agf__Release__r.agf__Application__r.Name   &lt;&gt; agf__Release_Approver__r.agf__Application__r.Name</errorConditionFormula>
        <errorDisplayField>agf__Release_Approver__c</errorDisplayField>
        <errorMessage>The Approver must be an Application Approver of the Application to which  this Signoff record&apos;s Release is related.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>agf__Either_Release_or_eRelease_Request</fullName>
        <active>true</active>
        <description>Either the Release field or the eRelease Request field must be specified, but not both.</description>
        <errorConditionFormula>(ISBLANK(agf__Release__c) &amp;&amp; ISBLANK(agf__eRelease_Request__c)) ||
  (NOT(ISBLANK(agf__Release__c)) &amp;&amp; NOT(ISBLANK(agf__eRelease_Request__c)))</errorConditionFormula>
        <errorMessage>Either Release must be specified or eRelease Request must be specified, but both cannot be entered.</errorMessage>
    </validationRules>
